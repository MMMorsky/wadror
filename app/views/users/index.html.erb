<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class="table table-hover">
  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user %></td>
        <td>
          <% if user.blocked %>
          Account Frozen
          <% end %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>

